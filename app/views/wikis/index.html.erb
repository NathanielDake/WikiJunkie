<div class="col-md-6">
<h1>All Wikis</h1>
<% @wikis.each do |wiki| %>
      <% if user_authorized_to_view_wiki?(wiki) %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
              <p><%= link_to wiki.title, wiki %></p>
          </h4>
        </div>
      </div>
      <% end %>
<% end %>
</div>
<div class="col-md-6">
  <%= link_to "New Wiki", new_wiki_path(@wiki), class: 'btn btn-success'%>
</div>
